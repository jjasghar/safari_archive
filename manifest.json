{
  "manifest_version": 3,
  "name": "Archive.ph Link Opener",
  "version": "1.0.0",
  "description": "Right-click on links to open them via archive.ph",
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}

